<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.group.voluntaryfilling.mapper.ScienceMapper">
    <select id="getScienceRank" resultType="com.group.voluntaryfilling.entity.vo.RankVo">
        select * from science where score=#{score};
    </select>

    <select id="getArtsRank" resultType="com.group.voluntaryfilling.entity.vo.RankVo">
        select * from liberal_arts where score=#{score};
    </select>

</mapper>
